  "emmet.includeLanguages": {
    "rust": "html"
  },

  "python.pythonPath": "/usr/bin/python3.7",
  "python.autoComplete.extraPaths": ["${workspaceFolder}/customModule"],
  "python.linting.pylintPath": "pylint",
  "python.linting.pylintArgs": ["--load-plugins", "pylint_django"],
  "python.linting.pylintEnabled": true,
  "python.linting.pycodestyleArgs": ["--ignore=E501"],
  "files.exclude": {
    "**/*.pyc": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.colorCustomizations": {
    
    "editor.background": "#faf7f5",
    "terminal.background": "#faf7f5",
    "sideBar.background": "#e7e2df"
  },
  "git.enableSmartCommit": true,
  "workbench.colorTheme": "GitHub Plus",
  "python.languageServer": "Pylance",
  "python.analysis.extraPaths": ["${workspaceFolder}/customModule"],
  "window.zoomLevel": 0,
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "julia.enableTelemetry": true,
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.requireConfig": true,

  "tailwindCSS.includeLanguages": {
    "javascript": "javascript",
    "html": "HTML",
    "plaintext": "rust",
    "rust": "rust"
  },

  "tailwindCSS.experimental.classRegex": [
    "class=\"([^\"]*)",
    "class={\"([^\"}]*)"
  ],

  "html-css-class-completion.HTMLLanguages": [
    "html",
    "vue",
    "razor",
    "blade",
    "handlebars",
    "twig",
    "django-html",
    "php",
    "markdown",
    "erb",
    "ejs",
    "svelte",
    "rust",
    "*.rs"
  ],

  "editor.quickSuggestions": {
    "strings": true
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/squashfs-root/usr/bin/nvim",
  "jupyter.textOutputLimit": 0,
  "editor.fontLigatures": null,
  "editor.codeActionsOnSave": null,
  "redhat.telemetry.enabled": false,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewFlutterUiGuides": true,
  "cmake.configureOnOpen": false,
  "python.defaultInterpreterPath": "/usr/bin/python3.7",
  "[rust]": {
    "editor.defaultFormatter": "statiolake.vscode-rustfmt"
  },
  "tabnine.experimentalAutoImports": true,

  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "svelte.enable-ts-plugin": true,
  "C_Cpp.updateChannel": "Insiders",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.associations": {
    "*.rs": "rust"
  },
  "editor.inlineSuggest.enabled": true
}
